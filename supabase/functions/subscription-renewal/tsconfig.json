{
  "compilerOptions": {
    "target": "es2021",
    "lib": ["es2021", "deno.ns"],
    "module": "es2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react@18.2.0"
  }
} 